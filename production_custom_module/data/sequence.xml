<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="seq_store_request_sequence" model="ir.sequence">
            <field name="name">store Issue Sequence</field>
            <field name="code">store.request</field>
            <field name="prefix">SIV/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>


    <record id="seq_fgrn_return" model="ir.sequence">
        <field name="name">FGRN Return</field>
        <field name="code">fgrn.return</field>
        <field name="prefix">FGRN/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>