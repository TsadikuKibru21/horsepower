<odoo>

    <record id="view_sales_from_tree" model="ir.ui.view">
        <field name="name">sales.from.tree</field>
        <field name="model">sales.from</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="product"/>
                <field name="reference"/>
                <field name="quantity"/>
                <field name="partner_id"/>
            </tree>
        </field>
    </record>

