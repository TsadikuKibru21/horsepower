<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sale_order_stock_wizard_form" model="ir.ui.view">
        <field name="name">sale.order.stock.wizard.form</field>
        <field name="model">sale.order.stock.wizard</field>
        <field name="arch" type="xml">
            <form string="Insufficient Stock Warning">
                <group>
                    <field name="message"/>
                    
                </group>
                <footer>
                    <button name="action_reserve_remaining" string="Reserve Remaining" type="object" class="btn-primary"/>
                    <button name="action_cancel" string="Cancel" type="object" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

     <record id="action_sale_order_stock_wizard_form" model="ir.actions.act_window">
        <field name="name">Reserve</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sale.order.stock.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>