<?xml version="1.0" encoding="utf-8"?>
<odoo>

 <data noupdate="1">
         <record id="quotation_followup_sequence" model="ir.sequence">
            <field name="name">quotation followup Sequence</field>
            <field name="code">quotation.followup.wizard</field>
            <field name="prefix">QFU/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <record id="view_quotation_followup_wizard_tree" model="ir.ui.view">
    <field name="name">quotation.followup.wizard.tree</field>
    <field name="model">quotation.followup.wizard</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="start_date"/>
            <field name="end_date"/>
        </tree>
    </field>
</record>
    <record id="view_quotation_followup_wizard_form" model="ir.ui.view">
        <field name="name">quotation.followup.wizard.form</field>
        <field name="model">quotation.followup.wizard</field>
        <field name="arch" type="xml">
            <form string="Quotation Follow-up Report">

            <header>
                <button name="action_generate_xlsx" string="Generate XLSX" type="object" class="btn-primary"/>
            </header>
                <sheet>
                    <group>
                    <field name="name" readonly="1"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                   
                     
                </sheet>
                   
            </form>
        </field>
    </record>

    <record id="action_quotation_followup_wizard" model="ir.actions.act_window">
        <field name="name">Quotation Follow-up Report</field>
        <field name="res_model">quotation.followup.wizard</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu item (optional; add to Sales menu) -->
    <menuitem id="menu_quotation_followup_report"
              name="Quotation Follow-up"
              parent="sale.menu_sale_report"
              action="action_quotation_followup_wizard"
              sequence="50"/>
</odoo>