<odoo>
    <data noupdate="1">
        <record id="seq_payment_order" model="ir.sequence">
            <field name="name">Payment Order</field>
            <field name="code">payment.order</field>
            <field name="prefix">PO/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_cheque_payment_voucher" model="ir.sequence">
            <field name="name">cheque payment voucher</field>
            <field name="code">cheque.payment.voucher</field>
            <field name="prefix">CPV/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
